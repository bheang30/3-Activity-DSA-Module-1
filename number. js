const firstFavNum = 6;
const secondFavNum = 9;
//Addition 
const sum = firstFaveNum + secondFavnum;
console.log(sum);
//Subtraction 
const difference = firstFavNum - secondFavNum
console.log(difference);
//Multiplication 
const product = firstFavNum * secondFavNum;
console.log(product);
//Division 
const qoutient = firstFavNum / secondFavNum;
console.log(quotient);
//Modulus
const reminder = firstFavNum % secondFavNum;
console.log(remainder);
//Exponentiation(Power)
const power = firstFavNum ** secondFavNum;
console.log(power);
